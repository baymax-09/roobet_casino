{
  "name": "roobet-fe",
  "version": "0.0.1",
  "description": "The frontend for roobet.com",
  "type:": "module",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "./src/app/util/windowHelpers.ts",
    "./src/common/icons.ts"
  ],
  "scripts": {
    "analyze:product": "cross-env BUILD_TARGET=product npm run build -- --analyze",
    "analyze:acp": "cross-env BUILD_TARGET=acp npm run build -- --analyze",
    "build": "cross-env NODE_ENV=production webpack --profile --mode=production",
    "build:dev": "cross-env NODE_ENV=development webpack --profile --mode=development",
    "checktypes": "tsc",
    "checktypes:ci": "node ./scripts/findSilencedFilesWithNoErrors.js",
    "dev": "cross-env NODE_ENV=development webpack serve --port 9000 --hot --mode=development",
    "dev:acp": "npm run dev -- --env BUILD_TARGET=acp --port 9001",
    "dev:docker:up": "docker compose up -d && sleep 1 && docker compose logs -t -f",
    "dev:storybook": "storybook dev -p 6006",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts --ext .tsx",
    "lint:error": "eslint --quiet src --ext .ts --ext .tsx",
    "lint:fix": "eslint --fix src --ext .ts --ext .tsx",
    "lint:yaml": "yamllint --no-warnings .",
    "format:check": "prettier --check -- 'src/**/*.{ts,tsx}'",
    "format:fix": "prettier --write -- 'src/**/*.{ts,tsx}'",
    "test": "npm run test:unit && npm run test:langKeys",
    "test:ci": "npm run test:unit",
    "test:langKeys": "i18next -s && node ./scripts/compare-lang-keys.js",
    "test:unit": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --max_old_space_size=4096\" jest",
    "ui:add": "yalc add @project-atl/ui",
    "ui:remove": "yalc remove @project-atl/ui"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --quiet --",
      "prettier --write --"
    ],
    "*.md": [
      "prettier --write --"
    ],
    "*.{yaml,yml}": [
      "yamllint --no-warnings"
    ]
  },
